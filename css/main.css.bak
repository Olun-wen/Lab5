main {
  padding: 100px;
  margin-left: auto;
  margin-right: auto;
  background-color: #d3f2f7;
  overflow: hidden;
}

caption {
  font-family: 'Poppins', sans-serif;
  font-size: 2em;
  font-weight: 700;
}

table {
  border: 1px solid #000;
  font-family: 'Poppins', sans-serif;
  border-collapse: collapse;
}

th {
  text-align: left;
  padding: 15px;
  font-weight: 900;
  border: 1px solid #000;
}

td {
  padding: 15px;
  text-align: left;
  border: 1px solid #000;
}

.tab-font1 {
  font-size: 1.5em;
  color: darkcyan;
  text-align: center;
}
 
.tab-font2 {
  font-size: 1.5em;
  color: #192270;
  text-align: center;
}